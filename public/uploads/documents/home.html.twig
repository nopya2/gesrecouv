                <b>EN CE MOMENT<b>
            </span>

            <span class="dis-inline-block cl6 slide100-txt pos-relative size-w-0 text-danger" data-in="fadeInDown" data-out="fadeOutDown">
                {% for flash in flashes|slice(0, 4) %}
                <span class="dis-inline-block slide100-txt-item animated visible-false">
                    <span class="label label-warning bg-warning p-2 text-light">
                        {{ flash.article.rubrique.titre }}
                    </span>
                    <a href="{{ path('article', {'article': flash.article.slug}) }}" style="margin: 0 5px 0 5px;">
                        {{ flash.contenu }}
                    </a>
                    <button class="btn btn-xs btn-light p-2">
                        <i class="fa fa-caret-left"></i>
                    </button>
                    <button class="btn btn-xs btn-light p-2">
